{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"typeRoots": ["./node_modules/@types", "@types"],
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"moduleResolution": "bundler"
	}

}
